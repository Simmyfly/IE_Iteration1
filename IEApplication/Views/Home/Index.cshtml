<section id="courses" class="section-padding">
    <div class="container" style="width:80%">
        <div class="row">
            <ul class="nav nav-tabs process-model more-icon-preocess" style="max-width:800px;margin-bottom:45px" role="tablist">
                <li role="presentation" class="active">
                    <a>
                        <i class="fa fa-search" aria-hidden="true"></i>
                        <p>1 Dental Problems</p>
                    </a>
                </li>
                <li role="presentation">
                    <a>
                        <i class="fa fa-clipboard" aria-hidden="true"></i>
                        <p>2 Problem Details</p>
                    </a>
                </li>
                <li role="presentation">
                    <a>
                        <i class="fa fa-qrcode" aria-hidden="true"></i>
                        <p>3 Category Selection</p>
                    </a>
                </li>
                <li role="presentation" id="priceTab">
                    <a href="#content">
                        <i class="fa fa-newspaper-o" aria-hidden="true"></i>
                        <p>4 Price and Budget</p>
                    </a>
                </li>
                <li role="presentation">
                    <a aria-controls="ClinicsMap" id="clinicsTab">
                        @*<a href="#reporting" aria-controls="reporting" role="tab" data-toggle="tab"/>*@
                        <i class="fa fa-send-o" aria-hidden="true"></i>
                        <p>5 Find Dental Clinics</p>
                    </a>
                </li>
            </ul>
            <div class="header-section text-center">
                <h2>Are you Suffering from any of these issues?</h2>

                <p></p>
                <hr class="bottom-line">
            </div>
        </div>
    </div>
    <div class="container" style="width:75%">
        <div class="row">
            <div class="col-md-3 col-sm-6 padleft-right">
                <div class="pm-staff-profile-details text-center" style="margin-bottom:0px;padding-bottom:5px;">
